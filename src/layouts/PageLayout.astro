---
import Typography from "~/components/Typography.astro";
import BaseLayout from "~/layouts/BaseLayout.astro";
import type { PageProps } from "~/types/Page";

type Props = PageProps;

const { title, twStyles, description, staticPage } = Astro.props;
---

<BaseLayout title={title} description={description} staticPage={staticPage}>
  <Typography twStyles={twStyles}>
    <h1>{title}</h1>
    <section>
      <slot />
    </section>
  </Typography>
</BaseLayout>
